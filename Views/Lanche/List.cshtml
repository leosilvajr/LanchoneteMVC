@*@model IEnumerable<Lanche>*@

    @*Instanciando a View Model que possui objetos de Lanches e Categorias*@
@model LancheListViewModel


<div>
    <h2>@Model.CategoriaAtual</h2>
</div>


<div class="container">
    <div class="row">


    @foreach (var lanche in Model.Lanches)
    {
        <partial name="_LanchesResumo" for="@lanche"/>
    }

    </div>
</div>